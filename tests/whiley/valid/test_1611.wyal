type char is (int x)
where:
    (0 <= x) && (x <= 255)


type State is ({char[] input, int pos} s)
where:
    s.pos >= 0


function isDigit(char c) -> (bool $)

assert "type invariant not satisfied":
    forall(State st):
        if:
            either:
                st.pos < |st.input|
                !isDigit(st.input[st.pos])
            or:
                st.pos >= |st.input|
        then:
            st is State


