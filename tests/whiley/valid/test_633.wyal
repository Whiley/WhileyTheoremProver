type fr5nat is (int x)
where:
    x >= 0


define g_loopinvariant_17(fr5nat[] xs, fr5nat[] ys, int i) is:
    (i >= 0) && (|xs| == |ys|)


assert "loop invariant not restored":
    forall(fr5nat[] xs, fr5nat[] ys, int i, fr5nat[] ys$1, int i$1, fr5nat[] ys$3, int i$2, fr5nat[] ys$2):
        if:
            ys == [0;|xs|]
            i == 0
            g_loopinvariant_17(xs, ys$1, i$1)
            i$1 < |xs|
            either:
                xs[i$1] > 1
                ys$2 == ys$1[i$1:=xs[i$1]]
                ys$3 == ys$2
            or:
                xs[i$1] <= 1
                ys$3 == ys$1
            i$2 == (i$1 + 1)
        then:
            g_loopinvariant_17(xs, ys$3, i$2)


