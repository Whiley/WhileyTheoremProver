type PieceKind is (int x)
where:
    (0 <= x) && (x <= 5)


type Piece is ({bool colour, PieceKind kind} self)


type Board is ({bool flag, Piece[] rows} self)


define f_requires_0(Board board) is:
    |board.rows| > 0


assert "index out of bounds (not less than length)":
    forall(Board board):
        if:
            f_requires_0(board)
        then:
            0 < |board.rows|


