define nat_a(int x) is:
    x >= 0


define nat_b(int x) is:
    x >= 0


function id(int x) -> (int y)

define id_requires_0(int x) is:
    nat_a(x)


define id_ensures_0(int x, int y) is:
    nat_b(y)


assert "precondition not satisfied":
    if:
        id_ensures_0(0, id(0))
        id(0) == 0
        id_ensures_0(1, id(1))
        id(1) == 1
    then:
        id_requires_0(2)


