type tac2ta is ({int f1, int f2} _this)
where:
    _this.f1 < _this.f2


type tac2tb is ({int f1, int f2} _this)
where:
    (_this.f1 + 1) < _this.f2


function f(tac2ta x) -> (tac2tb $)

assert "type invariant not satisfied":
    forall(tac2ta x$1, tac2ta x):
        if:
            x == {f1: 2, f2: 3}
            x == {f1: 2, f2: 3}
            x$1 == x{f1:=1}
        then:
            f(x$1) is tac2tb


