define add_requires_0(int x, int y) is:
    (x >= 0) && (y >= 0)


define add_ensures_0(int x, int y, int r) is:
    r > 0


assert "postcondition not satisfied":
    forall(int x, int y):
        if:
            add_requires_0(x, y)
            x != y
        then:
            add_ensures_0(x, y, x + y)


