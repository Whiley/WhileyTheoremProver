define Map<K,V> ({K,V} pairs) is:
  forall(K k1, V v1, K k2, V v2):
    if:
      (k1,v1) in pairs
      (k2,v2) in pairs
      k1 == k2
    then:
      v1 == v2

define Bijection({int,int} pairs) is:
  forall(int i1, int i2, int v1, int v2):
    if:
      (i1, v1) in pairs
      (i2, v2) in pairs
      (i1,v1) != (i2,v2)
    then:
      i1 != i2
      v1 != v2

assert:
  forall({int,int} pairs):
    Map<int,int>(pairs) ==> Bijection(pairs)


